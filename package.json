{
  "name": "swagger",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "yarn doc",
    "dev": "nodemon ./src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start": "node -r dotenv/config ./dist/server.js",
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
